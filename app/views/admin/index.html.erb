<div class="section">
    <div class="container cont">
        <div class="row">
            <div class="col-md-6">
            <h3>Waiting list</h3>
                <% @students.each do |s| %>
                <% if s.issued == false %>
                <tr>
                    <td><%= s.unmid %></td>
                    <td><%= s.email %></td>
                    <td><%= s.name %></td>
                    <td><%= link_to "Move to issued section" , admin_move_path(:id => s.id) , class: "btn btn-default", data: { confirm: "Are you sure?" }%></td>
                    <td><%= link_to "Delete" , admin_deletestudent_path(:id => s.id) ,:method => :delete, class: "btn btn-default", data: { confirm: "Are you sure?" }%></td>

                </tr><br>
                <% end %>
                <% end %>
            </div>
            <div class="col-md-6">
                <h3>Issued list</h3>
                <% @students.each do |s| %>
                <% if s.issued? %>
                <tr>
                    <td><%= s.unmid %></td>
                    <td><%= s.email %></td>
                    <td><%= s.name %></td>
                    <td><%= link_to "Move back to Waiting section" , admin_moveback_path(:id => s.id) , class: "btn btn-default", data: { confirm: "Are you sure?" }%></td>
                    <td><%= link_to "Delete" , admin_deletestudent_path(:id => s.id) ,:method => :delete, class: "btn btn-default", data: { confirm: "Are you sure?" }%></td>
                </tr><br>
                <% end %>
                <% end %>
            </div>
        </div>
    </div>
</div>